{
  "name": "wodisher",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:prod": "npx http-server -p 8080 -c-1 dist",
    "build": "npm version patch && vue-cli-service build && gulp build",
    "build:patch": "npm version patch && vue-cli-service build && gulp build",
    "build:minor": "npm version minor && vue-cli-service build && gulp build",
    "build:major": "npm version major && vue-cli-service build && gulp build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "core-js": "^3.4.3",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "lint-staged": "^9.4.3",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "vue": {
    "publicPath": "/wodisher/",
    "pwa": {
      "name": "Wodisher",
      "themeColor": "#121317",
      "msTileColor": "#121317",
      "appleMobileWebAppCapable": "yes",
      "workboxOptions": {
        "skipWaiting": true
      }
    }
  }
}
